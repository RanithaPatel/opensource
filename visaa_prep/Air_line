import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int x = sc.nextInt();
        int n = sc.nextInt();
        int maxPassengers = x*100;
        int planesRequired = (n-maxPassengers+99)/100;
        System.out.println(planesRequired);
        
            
    }
}
