import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        long n = sc.nextInt();
        int i = sc.nextInt();
        long bitMask = 1L << (i-1);
        if((n & bitMask) !=0){
            System.out.println("true");
        }else{
            System.out.println("false");
        }
        
    }
}
